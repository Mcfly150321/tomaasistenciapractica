{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/main.py"
    },
    {
      "source": "/docs",
      "destination": "/api/main.py"
    },
    {
      "source": "/openapi.json",
      "destination": "/api/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}